{
    "Serilog": {
      "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
      "MinimumLevel": "Debug",
      "WriteTo": [
        { "Name": "Console" },
        {
          "Name": "File",
          "Args": { "path": "Logs/log.txt" }
        }
      ],
      "Properties": {
        "Application": "Sample"
      }
    },
  
  "AllowedHosts": "*",
  "ReadMe": "Production Readme",
  "ConnectionString": [
    {
      "ConnStr": "prod.db.com:1521" // random, production
    }
  ]
}
